{"info": {"name": "Circular Object Detection API", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"}, "item": [{"name": "Images", "item": [{"name": "Upload Image", "request": {"method": "POST", "url": "{{base_url}}/upload", "body": {"mode": "formdata", "formdata": [{"key": "image", "type": "file", "src": ""}]}, "description": "Uploads an image to the server and stores it in the database."}}, {"name": "Get Images", "request": {"method": "GET", "url": "{{base_url}}/images", "description": "Retrieves a list of all images stored in the database."}}, {"name": "Get Image", "request": {"method": "GET", "url": "{{base_url}}/image/:image_id", "description": "Retrieves a specific image by its ID."}}, {"name": "Delete Image", "request": {"method": "DELETE", "url": "{{base_url}}/delete/:image_id", "description": "Deletes a specific image and its associated object detections from the database."}}]}, {"name": "Objects", "item": [{"name": "Get Objects", "request": {"method": "GET", "url": "{{base_url}}/objects/:image_id", "description": "Retrieves a list of all circular objects detected in a specific image."}}, {"name": "Get Object Details", "request": {"method": "GET", "url": "{{base_url}}/object/:object_id", "description": "Retrieves the details of a specific circular object."}}]}, {"name": "Processing", "item": [{"name": "Process Image", "request": {"method": "POST", "url": "{{base_url}}/process/:image_id", "body": {"mode": "raw", "raw": "{\"algorithm\": \"hough\"}", "options": {"raw": {"language": "json"}}}, "description": "Processes an image to detect and segment circular objects."}}]}]}